{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "project": {
    "name": "contact2"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "assets": [
        "assets",
        "favicon.ico",
        {
          "glob": "ext-tern.js",
          "input": "../node_modules/ng-ace-tern/ace-builds/src-noconflict/",
          "output": "./"
        },
        {
          "glob": "worker-tern.js",
          "input": "../node_modules/ng-ace-tern/ace-builds/src-noconflict/",
          "output": "./"
        }
      ],
      "index": "index.html",
      "main": "main.ts",
      "test": "test.ts",
      "tsconfig": "tsconfig.app.json",
      "testTsconfig": "tsconfig.spec.json",
      "prefix": "app",
      "polyfills": "polyfills.ts",
      "styles": [
        "styles.scss",
        "./assets/icons/icons.css",
        "../node_modules/font-awesome/css/font-awesome.css",
        "../node_modules/simple-line-icons/css/simple-line-icons.css",
        "../node_modules/summernote/dist/summernote.css"
      ],
      "scripts": [
        "../node_modules/jquery/dist/jquery.min.js",
        "../node_modules/ramda/dist/ramda.min.js",
        "../node_modules/bootstrap/dist/js/bootstrap.min.js",
        "../node_modules/summernote/dist/summernote.js",
        "../node_modules/leaflet/dist/leaflet.js",
        "../node_modules/ng-ace-tern/ace-builds/src-noconflict/ace.js",
        "../node_modules/ng-ace-tern/ace-builds/src-noconflict/snippets/javascript.js",
        "../node_modules/ng-ace-tern/ace-builds/src-noconflict/theme-eclipse.js",
        "./app/shared/components/form/script-editor/mode-groovy-tern.js"
      ],
      "environmentSource": "environments/environment.ts",
      "environments": {
        "dev": "environments/environment.ts",
        "prod": "environments/environment.prod.ts"
      }
    }
  ],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "lint": [
    {
      "project": "src/tsconfig.app.json"
    },
    {
      "project": "src/tsconfig.spec.json"
    },
    {
      "project": "e2e/tsconfig.e2e.json"
    }
  ],
  "defaults": {
    "styleExt": "scss",
    "class": {
      "spec": false
    },
    "component": {
      "spec": false
    },
    "directive": {
      "spec": false
    },
    "guard": {
      "spec": false
    },
    "service": {},
    "pipe": {},
    "interface": {
      "prefix": "I"
    }
  },
  "packageManager": "npm"
}
